{
  "global_response_headers": {
    "Connection": "keep-alive",
    "Keep-Alive": "timeout=5, max=100"
  },
  "access_control": {
    "deny_action": 404,
    "list": {
      "example1.html": "allow",
      "example2.html": "deny"
    }
  },
  "bind": "127.0.0.1:8080",
  "dynamic_pages": [
    "index"
  ],
  "dynamic_pages_library": "libdynamic_pages.so",
  "supported_encodings":  ["gzip", "br"],
  "use_encoding": "br",
  "document_root": "htdocs",
  "server_root": "/home/footer/RustroverProjects/web_server_poc",
  "https": {
    "enabled": true,
    "bind": "127.0.0.1:4443",
    "ssl_private_key_file": "key.pem",
    "ssl_certificate_file": "cert.pem"
  }
}